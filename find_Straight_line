#include<stdio.h>
int main()
{
int x1,x2,x3,y1,y2,y3,s;
printf("enter a value of coordinate (x1,y1):");
scanf("%d%d",&x1,&y1);
printf("enter a value of coordinate (x2,y2):");
scanf("%d%d",&x2,&y2);
printf("enter a value of coordinate (x3,y3):");
scanf("%d%d",&x3,&y3);
s=(x1*(y2-y3)+x2*(y3-y1)+x3*(y1-y2));
if(s==0)
{
  printf("points lies in straight line");
}
else
{
 printf("points not lies in straight line");
}
return 0;
}
